    <script type="text/javascript">
        function generateMeterResults() {
            var gaugeOptions = {

                chart: {
                    backgroundColor: '#EDEEFA',
                    type: 'solidgauge'

                },

                title: null,

                pane: {
                    center: ['50%', '85%'],
                    size: '140%',
                    startAngle: -90,
                    endAngle: 90,
                    background: {
                        backgroundColor: (Highcharts.theme && Highcharts.theme.background2) || '#EEE',
                        innerRadius: '60%',
                        outerRadius: '100%',
                        shape: 'arc'
                    }
                },

                tooltip: {
                    enabled: false
                },

                // the value axis
                yAxis: {
                    stops: [
                        [0.1, '#DF5353'], // red
                        [0.5, '#DDDF0D'], // yellow
                        [0.9, '#55BF3B']  // green
                    ],
                    lineWidth: 0,
                    minorTickInterval: null,
                    tickPixelInterval: 400,
                    tickWidth: 0,
                    title: {
                        y: -70
                    },
                    labels: {
                        y: 16
                    }
                },

                plotOptions: {
                    solidgauge: {
                        dataLabels: {
                            y: 5,
                            borderWidth: 0,
                            useHTML: true
                        }
                    }
                }
            };

            // The speed gauge
            $('#container-speed').highcharts(Highcharts.merge(gaugeOptions, {
                yAxis: {
                    min: 0,
                    max: 100,
                    title: {
                        text: 'M&A Prediction'

                    }
                },

                credits: {
                    enabled: false
                },

                series: [{
                    name: 'M&A Prediction',
                    data: [<%= finalPercent %>],
                    dataLabels: {
                        format: '<div style="text-align:center"><span style="font-size:25px;color:' +
                        ((Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black') + '">{y}</span><br/>' +
                        '<span style="font-size:12px;color:silver">%</span></div>'
                    },
                    tooltip: {
                        valueSuffix: '%'
                    }
                }]

            }));
        }


        $(function () {
            <%  if (data.length > 0 && data != 'undefined' && data != null && data) {

            console.log(JSON.stringify(data));
            %>

            $('#container1').highcharts({
                        chart: {
                            backgroundColor: '#EDEEFA',
                            type: 'column'
                        },
                        title: {
                            text: 'Stock Symbol: <%= comp1[0].Company_Name.toString().toUpperCase() %>'
                        },
                        /*  subtitle: {
                         text: 'Source: WorldClimate.com'
                         }, */

                        xAxis: {
                            categories: [
                                'Current Assests',
                                'Liability',
                                'Gross Profit',
                                'Total Revenue',
                                'EBITDA',
                                'Gross Margin',
                                'Cash Equivalent'

                            ],
                            crosshair: true
                        }
                        ,
                        yAxis: {
                            min: 0,
                            title: {
                                text: 'Amount (billions)'
                            }
                        }
                        ,
                        tooltip: {
                            headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                            pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                            '<td style="padding:0"><b>{point.y:.1f} bn</b></td></tr>',
                            footerFormat: '</table>',
                            shared: true,
                            useHTML: true
                        }
                        ,
                        plotOptions: {
                            column: {
                                pointPadding: 0,
                                borderWidth: 0
                            }
                        }
                        ,
                        series: [{

                            name: "<%= comp1[0].Year %>",
                            data: [
                                <%= comp1[0].Total_Current_Assets %>,
                                <%= comp1[0].Total_Liabilities %>,
                                <%= comp1[0].Gross_Margin %>,
                                <%= comp1[0].total_revenue %>,
                                <%= comp1[0].EBITDA %>,
                                <%= comp1[0].Cash_and_Cash_Equivalents %>
                            ]

                        }, {

                            name: "<%= comp1[1].Year %>",
                            data: [
                                <%= comp1[1].Total_Current_Assets %>,
                                <%= comp1[1].Total_Liabilities %>,
                                <%= comp1[1].Gross_Margin %>,
                                <%= comp1[1].total_revenue %>,
                                <%= comp1[1].EBITDA %>,
                                <%= comp1[1].Cash_and_Cash_Equivalents %>
                            ]
                        },
                            {

                                name: "<%= comp1[2].Year %>",
                                data: [
                                    <%= comp1[2].Total_Current_Assets %>,
                                    <%= comp1[2].Total_Liabilities %>,
                                    <%= comp1[2].Gross_Margin %>,
                                    <%= comp1[2].total_revenue %>,
                                    <%= comp1[2].EBITDA %>,
                                    <%= comp1[2].Cash_and_Cash_Equivalents %>
                                ]
                            }]
                    }
            );

            $('#container2').highcharts({
                        chart: {
                            backgroundColor: '#EDEEFA',
                            type: 'column'
                        },
                        title: {
                            text: 'Stock Symbol: <%= comp2[0].Company_Name.toString().toUpperCase() %>'
                        },
                        /*  subtitle: {
                         text: 'Source: WorldClimate.com'
                         }, */

                        xAxis: {
                            categories: [
                                'Current Assests',
                                'Liability',
                                'Gross Profit',
                                'Total Revenue',
                                'EBITDA',
                                'Gross Margin',
                                'Cash Equivalent'

                            ],
                            crosshair: true
                        }
                        ,
                        yAxis: {
                            min: 0,
                            title: {
                                text: 'Amount (billions)'
                            }
                        }
                        ,
                        tooltip: {
                            headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                            pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                            '<td style="padding:0"><b>{point.y:.1f} bn</b></td></tr>',
                            footerFormat: '</table>',
                            shared: true,
                            useHTML: true
                        }
                        ,
                        plotOptions: {
                            column: {
                                pointPadding: 0,
                                borderWidth: 0
                            }
                        }
                        ,
                        series: [{

                            name: "<%= comp2[0].Year %>",
                            data: [
                                <%= comp2[0].Total_Current_Assets %>,
                                <%= comp2[0].Total_Liabilities %>,
                                <%= comp2[0].Gross_Margin %>,
                                <%= comp2[0].total_revenue %>,
                                <%= comp2[0].EBITDA %>,
                                <%= comp2[0].Cash_and_Cash_Equivalents %>
                            ]

                        }, {

                            name: "<%= comp2[1].Year %>",
                            data: [
                                <%= comp2[1].Total_Current_Assets %>,
                                <%= comp2[1].Total_Liabilities %>,
                                <%= comp2[1].Gross_Margin %>,
                                <%= comp2[1].total_revenue %>,
                                <%= comp2[1].EBITDA %>,
                                <%= comp2[1].Cash_and_Cash_Equivalents %>
                            ]
                        },
                            {

                                name: "<%= comp2[2].Year %>",
                                data: [
                                    <%= comp2[2].Total_Current_Assets %>,
                                    <%= comp2[2].Total_Liabilities %>,
                                    <%= comp2[2].Gross_Margin %>,
                                    <%= comp2[2].total_revenue %>,
                                    <%= comp2[2].EBITDA %>,
                                    <%= comp2[2].Cash_and_Cash_Equivalents %>
                                ]
                            }]
                    }
            );

            <%}%>


            $('#container3').highcharts({
                chart: {
                    backgroundColor: '#EDEEFA',
                    type: 'pie',
                    options3d: {
                        enabled: true,
                        alpha: 45
                    }
                },
                title: {
                    text: 'Tweets Polarity'
                },
                subtitle: {
                    text: "[ <%= comp1[0].Company_Name.toString().toUpperCase() %>" + " + " + "<%= comp2[0].Company_Name.toString().toUpperCase() %> ]"
                },
                plotOptions: {
                    pie: {
                        innerSize: 100,
                        depth: 45
                    }
                },
                series: [{
                    name: 'Tweets Polarity',
                    data: [
                        ['Neutral', <%= polarity[2] %>],
                        ['Negative', <%= polarity[1] %>],
                        ['Positive', <%= polarity[0] %>]


                    ]
                }]
            });

        });

    </script>
