    <style type="text/css">
        .chartSize {
            height: 100%;
            width: 50%;
            position: relative;
        }

        .scrollit {
            overflow: scroll;
            height: 350px;
        }
    </style>


    <script type="text/javascript">

        $(function () {
            <%  if (data.length > 0 && data != 'undefined' && data != null && data) {

            %>

            $('#container1').highcharts({
                chart: {
                    type: 'column'
                },
                title: {
                    text: 'Net Debt. 2012 to 2014'
                },
                subtitle: {
                    text: 'Click the columns to view versions. Source: <a href="http://finance.yahoo.com">finance.yahoo.com</a>.'
                },
                xAxis: {
                    type: 'category'
                },
                yAxis: {
                    title: {
                        text: 'Debt in millions'
                    }

                },
                legend: {
                    enabled: false
                },
                plotOptions: {
                    series: {
                        borderWidth: 0,
                        dataLabels: {
                            enabled: true,
                            format: '{point.y:.1f}'
                        }
                    }
                },

                tooltip: {
                    headerFormat: '<span style="font-size:11px">{series.name}</span><br>',
                    pointFormat: '<span style="color:{point.color}">{point.name}</span>: <b>{point.y:.2f}%</b> of total<br/>'
                },

                series: [{
                    name: 'Company Net Debt data',
                    colorByPoint: true,
                    data: [{
                        name: 'Net Debt 2012',
                        y: parseInt('<%= comp1[0].Net_Debt %>'),
                        drilldown: 'Net Debt 2012'
                    }, {
                        name: 'Net Debt 2013',
                        y: parseInt('<%= comp1[1].Net_Debt %>'),
                        drilldown: 'Net Debt 2013'
                    }, {
                        name: 'Net Debt 2014',
                        y: parseInt('<%= comp1[2].Net_Debt %>'),
                        drilldown: 'Net Debt 2014'
                    }]
                }],
                drilldown: {
                    series: [{
                        name: 'Net Debt 2012',
                        id: 'Net Debt 2012',
                        data: [
                            [
                                'Short-Term Debt',
                                parseInt('<%= comp1[0].Short_Term_Debt %>')
                            ],
                            [
                                'Long-Term Debt',
                                parseInt('<%= comp1[0].Long_Term_Debt %>')
                            ]
                        ]
                    }, {
                        name: 'Net Debt 2013',
                        id: 'Net Debt 2013',
                        data: [
                            [
                                'Short-Term Debt',
                                parseInt('<%= comp1[1].Short_Term_Debt %>')
                            ],
                            [
                                'Long-Term Debt',
                                parseInt('<%= comp1[1].Long_Term_Debt %>')
                            ]
                        ]
                    }, {
                        name: 'Net Debt 2014',
                        id: 'Net Debt 2014',
                        data: [
                            [
                                'Short-Term Debt',
                                parseInt('<%= comp1[2].Short_Term_Debt %>')
                            ],
                            [
                                'Long-Term Debt',
                                parseInt('<%= comp1[2].Long_Term_Debt %>')
                            ]
                        ]
                    }]
                }
            });

            <%}%>

        });

    </script>

</head>

